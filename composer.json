{
    "name": "milleisole/flussu_server",
    "version": "4.5.1.20250930",
    "description": "The Flussu workflow-automation server.",
    "keywords": [
        "flussu",
        "server"
    ],
    "type": "project",
    "readmeFilename": "README.md",
    "license": "MIT",
    "authors": [
        {
            "name": "Aldo Prinzi",
            "email": "aldo@milleisole.com",
            "homepage": "https://www.milleisole.com",
            "role": "Main Developer"
        },
        {
            "name": "Concetto Pollicina",
            "email": "concetto@tuskcode.com",
            "homepage": "https://www.tuskcode.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "flussu@flussu.it",
        "github": "https://github.com/milleisole/flussu_open"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/milleisole/flussu_open.git"
    },
    "funding":[
        {
            "type": "other",
            "url": "https://www.flussu.com/sponsorship.html"
        } 
    ],
    "require": {
        "php": "^8.1",
        "ext-imagick": "^3.6",
        "ext-zip": "*",
        "claude-php/claude-3-api": "^0.1.23",
        "deepseek-php/deepseek-php-client": "^2.0",
        "dompdf/dompdf": "^2.0",
        "erusev/parsedown": "^1.7",
        "google-gemini-php/client": "^2.0",
        "google/apiclient": "^2.0",
        "guzzlehttp/guzzle": "^7.8.0",
        "guzzlehttp/psr7": "^2.6.1",
        "juanantoniodelgado/stopwords": "^1.3",
        "masterminds/html5": "^2.8",
        "nikic/php-parser": "^4.13",
        "openai-php/client": "^0.10.3",
        "phpmailer/phpmailer": "^6.5",
        "phpoffice/phpspreadsheet": "^2.1",
        "shuchkin/simplexlsxgen": "^1.2",
        "stripe/stripe-php": "^14.3",
        "symfony/browser-kit": "^5.4",
        "symfony/panther": "^2.2",
        "vlucas/phpdotenv": "^5.6",
        "web64/php-nlp-client": "^0.40.5",
        "zircote/swagger-php": "^3.0"
    },
    "autoload": {
        "classmap": [],
        "files": [
        ],
        "psr-4": {
             "Flussu\\": "src/Flussu/"
        }
    },
    "autoload-dev": {
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
        ],
        "post-create-project-cmd": [
        ],
        "post-autoload-dump": [
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "php-http/discovery": true
        },
        "platform-check": false,
        "platform": {
            "php": "8.1.0",
            "php-64bit": "8.1.0",
            "ext-json": "8.1",
            "ext-mbstring": "8.1"
        }
    },
    "require-dev": {
        "dbrekelmans/bdi": "^1.4"
    }
}
